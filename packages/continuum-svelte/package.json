{
  "name": "continuum-svelte",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./src/index.d.ts",
  "exports": {
    "types": "./src/index.d.ts",
    "import": "./dist/main.js",
    "require": "./dist/main.cjs"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --plugin-search-dir=. --write \"**/*.svelte\""
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.2",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "postcss-csso": "^6.0.1",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-variable-compress": "^2.0.1",
    "prettier": "latest",
    "prettier-plugin-svelte": "latest",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "svelte-preprocess": "^5.0.1",
    "terser": "^5.16.4",
    "tsconfig": "workspace:*",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.3",
    "vite-aliases": "^0.10.0"
  }
}
